{
    "*": {
        "box-sizing": "border-box"
    },
    "body": {
        "margin": "0"
    },
    "blockquote dl dd h1 h2 h3 h4 h5 h6 hr figure p pre": {
        "margin": "0"
    },
    "button": {
        "background-color": "transparent",
        "background-image": "none"
    },
    "fieldset": {
        "margin": "0",
        "padding": "0"
    },
    "ol ul": {
        "list-style": "none",
        "margin": "0",
        "padding": "0"
    }
}